# Production Environment Configuration Template
# Copy to .env and replace with real values

# Database (MySQL required for Plesk)
DATABASE_URL="mysql://user:password@localhost:3306/evercold_db"

# Node Environment
NODE_ENV=production
PORT=3000

# API Keys
YANDEX_MAPS_API_KEY=your_yandex_api_key_here
NEXT_PUBLIC_YANDEX_MAPS_API_KEY=your_yandex_api_key_here
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
CRON_SECRET=your_secure_random_string_here

# NextAuth (if used)
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=https://app.evercold.uz

# Production Settings
NEXT_TELEMETRY_DISABLED=1
